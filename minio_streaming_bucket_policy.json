{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "*"
                ]
            },
            "Action": [
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::streaming/*"
            ],
            "Condition": {
                "IpAddress": {
                    "aws:SourceIp": "127.0.0.1/32"
                }
            }
        }
    ]
}